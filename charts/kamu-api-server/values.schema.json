{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "properties": {
    "app": {
      "type": "object",
      "required": ["config"],
      "properties": {
        "config": {
          "type": "object",
          "required": ["datasetRepositoryUrl"],
          "properties": {
            "datasetRepositoryUrl": {
              "type": "string",
              "format": "uri"
            }
          }
        }
      }
    }
  }
}
