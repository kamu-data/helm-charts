{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "properties": {
    "app": {
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object",
          "required": [
            "datasetRepositoryUrl",
            "authGithubClientId",
            "authGithubClientSecret",
            "jwtSecret"
          ],
          "properties": {
            "datasetRepositoryUrl": {
              "type": "string",
              "format": "uri"
            },
            "authGithubClientId": {
              "type": "string"
            },
            "authGithubClientSecret": {
              "type": "string"
            },
            "jwtSecret": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}