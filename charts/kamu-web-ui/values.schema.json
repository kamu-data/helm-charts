{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "required": [
        "app"
    ],
    "properties": {
        "app": {
            "type": "object",
            "required": [
                "config"
            ],
            "properties": {
                "config": {
                    "type": "object",
                    "required": [
                        "apiServerGqlUrl",
                        "githubClientId",
                        "featureFlags"
                    ],
                    "properties": {
                        "apiServerGqlUrl": {
                            "type": "string",
                            "minLength": 1
                        },
                        "githubClientId": {
                            "type": "string",
                            "minLength": 1
                        },
                        "featureFlags": {
                            "type": "object",
                            "required": [
                                "enableLogout"
                            ],
                            "properties": {
                                "enableLogout": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}